/**index.wxss**/
.index-search {
  margin: 0 auto;
  border-radius: 30rpx;
  padding-left: 10rpx;
  width: 700rpx;
  height: 60rpx;
  background-color: #eee;
}


.swiper-item {
  width: 100%;
  height: 100%;
  text-align: center;
}


.index-footer {
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  border-top: 1rpx solid #eee;
  width: 100%;
  height: 100rpx;
  /* justify-content: space-between; */
}
.index-icont-font {
  font-size: 60rpx;
  line-height: 1;
}
.footer-active {
  color: #ffc0cb;
}

